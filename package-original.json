{
  "name": "my-fresh-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:update": "jest --updateSnapshot",
    "test:unit": "jest --testPathPattern='__tests__/(components|services|utils)/'",
    "test:integration": "jest --testPathPattern='__tests__/integration'",
    "test:e2e": "jest --testPathPattern='EndToEndScenarios'",
    "test:components": "jest --testPathPattern='components/__tests__'",
    "test:services": "jest --testPathPattern='services/__tests__'",
    "test:utils": "jest --testPathPattern='utils/__tests__'",
    "test:all": "npm run test:ci",
    "test:report": "node scripts/generate-test-report.js",
    "build:production": "cp app.production.json app.json && node ./scripts/build-production.js",
    "build:staging": "cp app.staging.json app.json && EXPO_PUBLIC_ENV=staging eas build --profile preview",
    "build:development": "cp app.json app.json && EXPO_PUBLIC_ENV=development eas build --profile development",
    "postbuild": "git checkout app.json",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "deploy:production": "npm run build:production && npm run submit:ios && npm run submit:android"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/elements": "^2.5.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "base-64": "^1.0.0",
    "bcrypt": "^6.0.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "expo": "~53.0.17",
    "expo-blur": "~14.1.5",
    "expo-camera": "^16.1.10",
    "expo-constants": "~17.1.7",
    "expo-crypto": "^14.1.5",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.2",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "^16.0.5",
    "expo-location": "^18.1.6",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.3",
    "expo-secure-store": "^14.2.3",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "^0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keychain": "^10.0.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "^13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/react": "~19.0.10",
    "babel-jest": "^29.7.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
